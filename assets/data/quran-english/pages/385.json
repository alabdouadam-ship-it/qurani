        "hizbQuarter": 154,
        "sajda": false
      },
      {
        "number": 3254,
        "text": "These are the verses of the illuminating Book.",
        "surah": {
          "number": 28,
          "name": "سُورَةُ القَصَصِ",
          "englishName": "Al-Qasas",
          "englishNameTranslation": "The Stories",
          "revelationType": "Meccan",
          "numberOfAyahs": 88
        },
        "numberInSurah": 2,
        "juz": 20,
