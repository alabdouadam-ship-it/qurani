        "ruku": 334,
        "hizbQuarter": 154,
        "sajda": false
      },
      {
        "number": 3255,
        "text": "We narrate to you from the history of Moses and Pharaoh in all verity, for those who believe.",
        "surah": {
          "number": 28,
          "name": "سُورَةُ القَصَصِ",
          "englishName": "Al-Qasas",
          "englishNameTranslation": "The Stories",
          "revelationType": "Meccan",
          "numberOfAyahs": 88
        },
        "numberInSurah": 3,
        "juz": 20,
        "manzil": 5,
