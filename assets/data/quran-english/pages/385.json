        "hizbQuarter": 154,
        "sajda": false
      },
      {
        "number": 3250,
        "text": "(Say): \"I am commanded to worship the Lord of this land He has blessed, to whom all things belong; and I am commanded to be one of those who submit,",
        "surah": {
          "number": 27,
          "name": "سُورَةُ النَّمۡلِ",
          "englishName": "An-Naml",
          "englishNameTranslation": "The Ant",
          "revelationType": "Meccan",
          "numberOfAyahs": 93
        },
        "numberInSurah": 91,
        "juz": 20,
