    "ayahs": [
      {
        "number": 3347,
        "text": "We shall pardon the sinful deeds of those who believe and do the right, and give them a reward better than their deeds.",
        "surah": {
          "number": 29,
          "name": "سُورَةُ العَنكَبُوتِ",
          "englishName": "Al-Ankaboot",
          "englishNameTranslation": "The Spider",
          "revelationType": "Meccan",
          "numberOfAyahs": 69
        },
        "numberInSurah": 7,
        "juz": 20,
        "manzil": 5,
        "page": 397,
