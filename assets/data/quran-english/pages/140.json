    "ayahs": [
      {
        "number": 884,
        "text": "Indeed it is God who splits up the seed and the kernel, and brings forth the living from the dead, the dead from the living. This is God: So whither do you stray?",
        "surah": {
          "number": 6,
          "name": "سُورَةُ الأَنۡعَامِ",
          "englishName": "Al-An'aam",
          "englishNameTranslation": "The Cattle",
          "revelationType": "Meccan",
          "numberOfAyahs": 165
        },
        "numberInSurah": 95,
        "juz": 7,
        "manzil": 2,
        "page": 140,
