    "ayahs": [
      {
        "number": 4126,
        "text": "When the trumpet blast is sounded whoever is in the heavens and the earth will swoon away, except those God please. When the blast is sounded the second time, they will stand up all expectant.",
        "surah": {
          "number": 39,
          "name": "سُورَةُ الزُّمَرِ",
          "englishName": "Az-Zumar",
          "englishNameTranslation": "The Groups",
          "revelationType": "Meccan",
          "numberOfAyahs": 75
        },
        "numberInSurah": 68,
