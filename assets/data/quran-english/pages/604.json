        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6234,
        "text": "From the evil of him who breathes temptations into the minds of men,",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 4,
        "juz": 30,
