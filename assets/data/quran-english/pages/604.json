        },
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6232,
        "text": "The King of men,",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6233,
        "text": "The God of men,",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 3,
        "juz": 30,
