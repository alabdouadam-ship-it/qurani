        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6235,
        "text": "Who suggests evil thoughts to the hearts of men --",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
