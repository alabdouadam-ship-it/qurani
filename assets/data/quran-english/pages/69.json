        "sajda": false
      },
      {
        "number": 443,
        "text": "But God is your protector, and He is the best of helpers.",
        "surah": {
          "number": 3,
          "name": "سُورَةُ آلِ عِمۡرَانَ",
          "englishName": "Aal-i-Imraan",
          "englishNameTranslation": "The Family of Imraan",
          "revelationType": "Medinan",
          "numberOfAyahs": 200
        },
        "numberInSurah": 150,
        "juz": 4,
        "manzil": 1,
        "page": 69,
