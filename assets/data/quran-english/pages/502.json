    "ayahs": [
      {
        "number": 4506,
        "text": "The evil of what they had done will become clear to them, and they will be seized by what they had scorned.",
        "surah": {
          "number": 45,
          "name": "سُورَةُ الجَاثِيَةِ",
          "englishName": "Al-Jaathiya",
          "englishNameTranslation": "Crouching",
          "revelationType": "Meccan",
          "numberOfAyahs": 37
        },
        "numberInSurah": 33,
