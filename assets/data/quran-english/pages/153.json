    "ayahs": [
      {
        "number": 977,
        "text": "They said: \"O our Lord, we have wronged ourselves. If You do not forgive us and have mercy upon us, we shall certainly be lost.\"",
        "surah": {
          "number": 7,
          "name": "سُورَةُ الأَعۡرَافِ",
          "englishName": "Al-A'raaf",
          "englishNameTranslation": "The Heights",
          "revelationType": "Meccan",
          "numberOfAyahs": 206
        },
        "numberInSurah": 23,
        "juz": 8,
        "manzil": 2,
        "page": 153,
