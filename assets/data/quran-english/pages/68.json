    "ayahs": [
      {
        "number": 434,
        "text": "This is so that God may try the faithful and destroy the unbelievers.",
        "surah": {
          "number": 3,
          "name": "سُورَةُ آلِ عِمۡرَانَ",
          "englishName": "Aal-i-Imraan",
