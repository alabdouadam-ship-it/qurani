      {
        "number": 1006,
        "text": "Indeed We had brought to them a Book distinct, replete with knowledge, and guidance and grace for men who believe.",
        "surah": {
          "number": 7,
          "name": "سُورَةُ الأَعۡرَافِ",
          "englishName": "Al-A'raaf",
          "englishNameTranslation": "The Heights",
          "revelationType": "Meccan",
          "numberOfAyahs": 206
        },
        "numberInSurah": 52,
        "juz": 8,
        "manzil": 2,
        "page": 157,
