        "hizbQuarter": 98,
        "sajda": false
      },
      {
        "number": 1693,
        "text": "Said (the sons): \"O our father, pray for us that our sins be forgiven, for we are really sinners.\"",
        "surah": {
          "number": 12,
          "name": "سُورَةُ يُوسُفَ",
          "englishName": "Yusuf",
          "englishNameTranslation": "Joseph",
          "revelationType": "Meccan",
          "numberOfAyahs": 111
        },
