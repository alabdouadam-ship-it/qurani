      {
        "number": 3296,
        "text": "You were not there on the western side (of Mount Sinai) when We gave the commandments to Moses, nor were you witness (to the event).",
        "surah": {
          "number": 28,
          "name": "سُورَةُ القَصَصِ",
          "englishName": "Al-Qasas",
          "englishNameTranslation": "The Stories",
          "revelationType": "Meccan",
          "numberOfAyahs": 88
        },
        "numberInSurah": 44,
        "juz": 20,
        "manzil": 5,
        "page": 391,
