        "number": 1675,
        "text": "\"May God forgive us,\" he said, \"if we hold any one but him with whom we found our property, or else we would be unjust.\"",
        "surah": {
          "number": 12,
          "name": "سُورَةُ يُوسُفَ",
          "englishName": "Yusuf",
          "englishNameTranslation": "Joseph",
          "revelationType": "Meccan",
          "numberOfAyahs": 111
        },
        "numberInSurah": 79,
