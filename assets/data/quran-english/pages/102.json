      {
        "number": 641,
        "text": "God does not like ill (of others) spoken about, except by him who has been wronged. For God hears all and knows everything.",
        "surah": {
          "number": 4,
          "name": "سُورَةُ النِّسَاءِ",
          "englishName": "An-Nisaa",
          "englishNameTranslation": "The Women",
          "revelationType": "Medinan",
          "numberOfAyahs": 176
        },
        "numberInSurah": 148,
        "juz": 6,
        "manzil": 1,
        "page": 102,
