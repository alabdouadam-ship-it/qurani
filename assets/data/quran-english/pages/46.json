      {
        "number": 277,
        "text": "Whatsoever you give away in alms or vow as offering, is all known to God; but the wicked will have none to help them.",
        "surah": {
          "number": 2,
          "name": "سُورَةُ البَقَرَةِ",
          "englishName": "Al-Baqara",
          "englishNameTranslation": "The Cow",
          "revelationType": "Medinan",
          "numberOfAyahs": 286
        },
        "numberInSurah": 270,
        "juz": 3,
        "manzil": 1,
