    "ayahs": [
      {
        "number": 5143,
        "text": "Both of them will end up in Hell, where they will abide for ever. This is the punishment for the wicked.",
        "surah": {
          "number": 59,
          "name": "سُورَةُ الحَشۡرِ",
          "englishName": "Al-Hashr",
          "englishNameTranslation": "The Exile",
          "revelationType": "Medinan",
          "numberOfAyahs": 24
        },
        "numberInSurah": 17,
        "juz": 28,
        "manzil": 7,
        "page": 548,
