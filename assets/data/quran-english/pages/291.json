    "ayahs": [
      {
        "number": 2116,
        "text": "Unless your Lord have mercy. His blessings on you are great indeed.",
        "surah": {
          "number": 17,
          "name": "سُورَةُ الإِسۡرَاءِ",
          "englishName": "Al-Israa",
          "englishNameTranslation": "The Night Journey",
          "revelationType": "Meccan",
          "numberOfAyahs": 111
        },
        "numberInSurah": 87,
        "juz": 15,
        "manzil": 4,
        "page": 291,
