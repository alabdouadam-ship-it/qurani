    "ayahs": [
      {
        "number": 3664,
        "text": "If they call you a liar, so have other messengers been denied before you. But all things will be brought back to God.",
        "surah": {
          "number": 35,
          "name": "سُورَةُ فَاطِرٍ",
          "englishName": "Faatir",
          "englishNameTranslation": "The Originator",
          "revelationType": "Meccan",
          "numberOfAyahs": 45
        },
        "numberInSurah": 4,
        "juz": 22,
        "manzil": 5,
        "page": 435,
