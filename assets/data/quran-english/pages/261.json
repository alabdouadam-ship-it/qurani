        "ruku": 217,
        "hizbQuarter": 104,
        "sajda": false
      },
      {
        "number": 1800,
        "text": "Of molten pitch shall be their garments, their faces covered with flames,",
        "surah": {
          "number": 14,
          "name": "سُورَةُ إِبۡرَاهِيمَ",
          "englishName": "Ibrahim",
          "englishNameTranslation": "Abraham",
