        "page": 261,
        "ruku": 217,
        "hizbQuarter": 104,
        "sajda": false
      },
      {
        "number": 1799,
        "text": "You will see the wicked on that day bound together in chains.",
        "surah": {
          "number": 14,
          "name": "سُورَةُ إِبۡرَاهِيمَ",
          "englishName": "Ibrahim",
          "englishNameTranslation": "Abraham",
