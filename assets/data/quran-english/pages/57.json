    "ayahs": [
      {
        "number": 346,
        "text": "\"O Lord, we believe in Your revelations and follow this Apostle. Enroll us among the witnesses.\"",
        "surah": {
          "number": 3,
          "name": "سُورَةُ آلِ عِمۡرَانَ",
          "englishName": "Aal-i-Imraan",
          "englishNameTranslation": "The Family of Imraan",
          "revelationType": "Medinan",
          "numberOfAyahs": 200
        },
        "numberInSurah": 53,
        "juz": 3,
        "manzil": 1,
        "page": 57,
