        "surah": {
          "number": 80,
          "name": "سُورَةُ عَبَسَ",
          "englishName": "Abasa",
          "englishNameTranslation": "He frowned",
          "revelationType": "Meccan",
          "numberOfAyahs": 42
        },
        "numberInSurah": 14,
        "juz": 30,
