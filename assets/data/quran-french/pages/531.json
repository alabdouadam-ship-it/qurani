        "page": 531,
        "ruku": 465,
        "hizbQuarter": 212,
        "sajda": false
      },
      {
        "number": 4902,
        "text": "Le Tout Miséricordieux.",
        "surah": {
          "number": 55,
          "name": "سُورَةُ الرَّحۡمَٰن",
          "englishName": "Ar-Rahmaan",
          "englishNameTranslation": "The Beneficent",
