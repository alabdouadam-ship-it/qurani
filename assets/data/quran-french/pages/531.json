        "hizbQuarter": 213,
        "sajda": false
      },
      {
        "number": 4914,
        "text": "Lequel donc des bienfaits de votre Seigneur nierez-vous?",
        "surah": {
          "number": 55,
          "name": "سُورَةُ الرَّحۡمَٰن",
          "englishName": "Ar-Rahmaan",
          "englishNameTranslation": "The Beneficent",
