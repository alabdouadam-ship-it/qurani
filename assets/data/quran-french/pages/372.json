        "hizbQuarter": 149,
        "sajda": false
      },
      {
        "number": 3067,
        "text": "Je crains pour vous le châtiment d'un Jour terrible».",
        "surah": {
          "number": 26,
          "name": "سُورَةُ الشُّعَرَاءِ",
          "englishName": "Ash-Shu'araa",
          "englishNameTranslation": "The Poets",
          "revelationType": "Meccan",
