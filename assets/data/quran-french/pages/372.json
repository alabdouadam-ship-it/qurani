        "page": 372,
        "ruku": 322,
        "hizbQuarter": 149,
        "sajda": false
      },
      {
        "number": 3066,
        "text": "de jardins et de sources.",
        "surah": {
          "number": 26,
          "name": "سُورَةُ الشُّعَرَاءِ",
          "englishName": "Ash-Shu'araa",
          "englishNameTranslation": "The Poets",
          "revelationType": "Meccan",
          "numberOfAyahs": 227
        },
        "numberInSurah": 134,
        "juz": 19,
