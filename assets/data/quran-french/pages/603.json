        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6212,
        "text": "Et vous n'êtes pas adorateurs de ce que j'adore.",
        "surah": {
          "number": 109,
          "name": "سُورَةُ الكَافِرُونَ",
          "englishName": "Al-Kaafiroon",
          "englishNameTranslation": "The Disbelievers",
