        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6209,
        "text": "Je n'adore pas ce que vous adorez.",
        "surah": {
          "number": 109,
          "name": "سُورَةُ الكَافِرُونَ",
          "englishName": "Al-Kaafiroon",
          "englishNameTranslation": "The Disbelievers",
