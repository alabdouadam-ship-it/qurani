        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6213,
        "text": "A vous votre religion, et à moi ma religion».",
        "surah": {
          "number": 109,
          "name": "سُورَةُ الكَافِرُونَ",
          "englishName": "Al-Kaafiroon",
          "englishNameTranslation": "The Disbelievers",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 6,
        "juz": 30,
