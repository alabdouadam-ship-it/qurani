        "ruku": 553,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6220,
        "text": "de même sa femme, la porteuse de bois,",
        "surah": {
          "number": 111,
          "name": "سُورَةُ المَسَدِ",
          "englishName": "Al-Masad",
          "englishNameTranslation": "The Palm Fibre",
          "revelationType": "Meccan",
