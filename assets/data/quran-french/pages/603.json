        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6211,
        "text": "Je ne suis pas adorateur de ce que vous adorez.",
        "surah": {
          "number": 109,
          "name": "سُورَةُ الكَافِرُونَ",
          "englishName": "Al-Kaafiroon",
          "englishNameTranslation": "The Disbelievers",
