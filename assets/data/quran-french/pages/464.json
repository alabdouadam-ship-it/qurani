    "ayahs": [
      {
        "number": 4106,
        "text": "et leur apparaîtront les méfaits qu'ils ont commis, et ce dont ils se raillaient les enveloppera.",
        "surah": {
          "number": 39,
          "name": "سُورَةُ الزُّمَرِ",
          "englishName": "Az-Zumar",
          "englishNameTranslation": "The Groups",
          "revelationType": "Meccan",
