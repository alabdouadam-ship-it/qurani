    "ayahs": [
      {
        "number": 3355,
        "text": "Puis Nous le sauvâmes, lui et les gens de l'arche; et Nous en fîmes un avertissement pour l'univers.",
        "surah": {
          "number": 29,
          "name": "سُورَةُ العَنكَبُوتِ",
          "englishName": "Al-Ankaboot",
          "englishNameTranslation": "The Spider",
          "revelationType": "Meccan",
          "numberOfAyahs": 69
        },
        "numberInSurah": 15,
