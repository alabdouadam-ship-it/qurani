    "ayahs": [
      {
        "number": 599,
        "text": "Et implore d'Allah le pardon car Allah est certes Pardonneur et Miséricordieux.",
        "surah": {
          "number": 4,
          "name": "سُورَةُ النِّسَاءِ",
          "englishName": "An-Nisaa",
          "englishNameTranslation": "The Women",
          "revelationType": "Medinan",
          "numberOfAyahs": 176
        },
        "numberInSurah": 106,
