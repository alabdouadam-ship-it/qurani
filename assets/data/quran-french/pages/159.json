    "ayahs": [
      {
        "number": 1022,
        "text": "Je vous communique les messages de mon Seigneur, et je suis pour vous un conseiller digne de confiance.",
        "surah": {
          "number": 7,
          "name": "سُورَةُ الأَعۡرَافِ",
          "englishName": "Al-A'raaf",
          "englishNameTranslation": "The Heights",
          "revelationType": "Meccan",
