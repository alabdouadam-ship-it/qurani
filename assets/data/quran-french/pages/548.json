    "ayahs": [
      {
        "number": 5143,
        "text": "Ils eurent pour destinée d'être tous deux dans le Feu pour y demeurer éternellement. Telle est la rétribution des injustes.",
        "surah": {
          "number": 59,
          "name": "سُورَةُ الحَشۡرِ",
          "englishName": "Al-Hashr",
          "englishNameTranslation": "The Exile",
          "revelationType": "Medinan",
          "numberOfAyahs": 24
        },
        "numberInSurah": 17,
        "juz": 28,
        "manzil": 7,
        "page": 548,
