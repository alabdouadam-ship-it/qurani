      {
        "number": 520,
        "text": "Et Allah veut accueillir votre repentir. Mais ceux qui suivent les passions veulent que vous vous incliniez grandement (vers l'erreur comme ils le font).",
        "surah": {
          "number": 4,
          "name": "سُورَةُ النِّسَاءِ",
          "englishName": "An-Nisaa",
          "englishNameTranslation": "The Women",
          "revelationType": "Medinan",
          "numberOfAyahs": 176
        },
        "numberInSurah": 27,
        "juz": 5,
        "manzil": 1,
        "page": 83,
