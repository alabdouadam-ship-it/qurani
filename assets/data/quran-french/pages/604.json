        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6224,
        "text": "Il n'a jamais engendré, n'a pas été engendré non plus.",
        "surah": {
          "number": 112,
          "name": "سُورَةُ الإِخۡلَاصِ",
          "englishName": "Al-Ikhlaas",
          "englishNameTranslation": "Sincerity",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 3,
        "juz": 30,
