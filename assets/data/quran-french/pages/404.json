        },
        "numberInSurah": 1,
        "juz": 21,
        "manzil": 5,
        "page": 404,
        "ruku": 349,
        "hizbQuarter": 162,
        "sajda": false
      },
      {
        "number": 3411,
        "text": "Les Romains ont été vaincus,",
        "surah": {
          "number": 30,
          "name": "سُورَةُ الرُّومِ",
          "englishName": "Ar-Room",
          "englishNameTranslation": "The Romans",
          "revelationType": "Meccan",
          "numberOfAyahs": 60
        },
        "numberInSurah": 2,
        "juz": 21,
