        "hizbQuarter": 23,
        "sajda": false
      },
      {
        "number": 356,
        "text": "Si donc ils tournent le dos... alors Allah connaît bien les semeurs de corruption!",
        "surah": {
          "number": 3,
          "name": "سُورَةُ آلِ عِمۡرَانَ",
          "englishName": "Aal-i-Imraan",
          "englishNameTranslation": "The Family of Imraan",
          "revelationType": "Medinan",
          "numberOfAyahs": 200
        },
        "numberInSurah": 63,
        "juz": 3,
        "manzil": 1,
        "page": 58,
