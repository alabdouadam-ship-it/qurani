    "ayahs": [
      {
        "number": 3664,
        "text": "Et s'ils te traitent de menteur, certes on a traité de menteurs des Messagers avant toi. Vers Allah cependant, tout est ramené.",
        "surah": {
          "number": 35,
          "name": "سُورَةُ فَاطِرٍ",
          "englishName": "Faatir",
          "englishNameTranslation": "The Originator",
          "revelationType": "Meccan",
          "numberOfAyahs": 45
        },
        "numberInSurah": 4,
        "juz": 22,
        "manzil": 5,
