        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3912,
        "text": "Quand il dit à son peuple: «Ne craignez-vous pas [Allah]?»",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
          "englishName": "As-Saaffaat",
          "englishNameTranslation": "Those drawn up in Ranks",
          "revelationType": "Meccan",
          "numberOfAyahs": 182
        },
        "numberInSurah": 124,
