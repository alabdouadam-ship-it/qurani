        "ruku": 389,
        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3913,
        "text": "Invoquerez-vous Ball (une idole) et délaisserez-vous le Meilleur des créateurs,",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
          "englishName": "As-Saaffaat",
          "englishNameTranslation": "Those drawn up in Ranks",
          "revelationType": "Meccan",
