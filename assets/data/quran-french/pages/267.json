    "ayahs": [
      {
        "number": 1893,
        "text": "ceux qui ont fait du Coran des fractions diverses, (pour créer des doutes).",
        "surah": {
          "number": 15,
          "name": "سُورَةُ الحِجۡرِ",
          "englishName": "Al-Hijr",
          "englishNameTranslation": "The Rock",
