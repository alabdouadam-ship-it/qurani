        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2775,
        "text": "فمن كثرت حسناته وثَقُلَتْ بها موازين أعماله عند الحساب، فأولئك هم الفائزون بالجنة.",
        "surah": {
          "number": 23,
          "name": "سُورَةُ المُؤۡمِنُونَ",
          "englishName": "Al-Muminoon",
          "englishNameTranslation": "The Believers",
          "revelationType": "Meccan",
          "numberOfAyahs": 118
        },
        "numberInSurah": 102,
