        "hizbQuarter": 139,
        "sajda": false
      },
      {
        "number": 2765,
        "text": "هو وحده يعلم ما غاب عن خلقه وما شاهدوه، فتنزَّه الله تعالى عن الشريك الذي يزعمون.",
        "surah": {
          "number": 23,
          "name": "سُورَةُ المُؤۡمِنُونَ",
          "englishName": "Al-Muminoon",
          "englishNameTranslation": "The Believers",
          "revelationType": "Meccan",
          "numberOfAyahs": 118
        },
        "numberInSurah": 92,
        "juz": 18,
