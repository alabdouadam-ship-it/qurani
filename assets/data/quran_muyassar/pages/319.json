        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2449,
        "text": "خالدين في العذاب، وساءهم ذلك الحمل الثقيل من الآثام حيث أوردهم النار.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 101,
        "juz": 16,
