        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4446,
        "text": "ولقد اصطفينا بني إسرائيل على عِلْم منا بهم على عالمي زمانهم.",
        "surah": {
          "number": 44,
          "name": "سُورَةُ الدُّخَانِ",
          "englishName": "Ad-Dukhaan",
          "englishNameTranslation": "The Smoke",
          "revelationType": "Meccan",
