        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4447,
        "text": "وآتيناهم من المعجزات على يد موسى ما فيه ابتلاؤهم واختبارهم رخاء وشدة.",
        "surah": {
          "number": 44,
          "name": "سُورَةُ الدُّخَانِ",
          "englishName": "Ad-Dukhaan",
          "englishNameTranslation": "The Smoke",
          "revelationType": "Meccan",
          "numberOfAyahs": 59
        },
        "numberInSurah": 33,
        "juz": 25,
