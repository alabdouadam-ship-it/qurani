        "hizbQuarter": 199,
        "sajda": false
      },
      {
        "number": 4445,
        "text": "من فرعون، إنه كان جبارًا من المشركين، مسرفًا في العلو والتكبر على عباد الله.",
        "surah": {
          "number": 44,
          "name": "سُورَةُ الدُّخَانِ",
          "englishName": "Ad-Dukhaan",
          "englishNameTranslation": "The Smoke",
          "revelationType": "Meccan",
