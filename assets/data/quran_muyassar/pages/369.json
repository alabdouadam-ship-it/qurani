        "hizbQuarter": 147,
        "sajda": false
      },
      {
        "number": 2974,
        "text": "قال فرعون: نعم لكم عندي ما طلبتم مِن أجر، وإنكم حينئذ لمن المقربين لديَّ.",
        "surah": {
          "number": 26,
          "name": "سُورَةُ الشُّعَرَاءِ",
          "englishName": "Ash-Shu'araa",
          "englishNameTranslation": "The Poets",
          "revelationType": "Meccan",
          "numberOfAyahs": 227
        },
        "numberInSurah": 42,
