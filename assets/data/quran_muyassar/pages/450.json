        "hizbQuarter": 180,
        "sajda": false
      },
      {
        "number": 3899,
        "text": "إنه من عبادنا المؤمنين الذين أعطَوا العبودية حقها.",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
