    "ayahs": [
      {
        "number": 6222,
        "text": "قل -أيها الرسول-: هو الله المتفرد بالألوهية والربوبية والأسماء والصفات، لا يشاركه أحد فيها.",
        "surah": {
          "number": 112,
          "name": "سُورَةُ الإِخۡلَاصِ",
          "englishName": "Al-Ikhlaas",
          "englishNameTranslation": "Sincerity",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6223,
        "text": "الله وحده المقصود في قضاء الحوائج والرغائب.",
        "surah": {
          "number": 112,
          "name": "سُورَةُ الإِخۡلَاصِ",
          "englishName": "Al-Ikhlaas",
          "englishNameTranslation": "Sincerity",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6224,
        "text": "ليس له ولد ولا والد ولا صاحبة.",
        "surah": {
          "number": 112,
          "name": "سُورَةُ الإِخۡلَاصِ",
          "englishName": "Al-Ikhlaas",
          "englishNameTranslation": "Sincerity",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6225,
        "text": "ولم يكن له مماثلا ولا مشابهًا أحد من خلقه، لا في أسمائه ولا في صفاته، ولا في أفعاله، تبارك وتعالى وتقدَّس.",
        "surah": {
          "number": 112,
          "name": "سُورَةُ الإِخۡلَاصِ",
          "englishName": "Al-Ikhlaas",
          "englishNameTranslation": "Sincerity",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 554,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6226,
        "text": "قل -أيها الرسول-: أعوذ وأعتصم برب الفلق، وهو الصبح.",
        "surah": {
          "number": 113,
          "name": "سُورَةُ الفَلَقِ",
          "englishName": "Al-Falaq",
          "englishNameTranslation": "The Dawn",
          "revelationType": "Meccan",
          "numberOfAyahs": 5
        },
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6227,
        "text": "من شر جميع المخلوقات وأذاها.",
        "surah": {
          "number": 113,
          "name": "سُورَةُ الفَلَقِ",
          "englishName": "Al-Falaq",
          "englishNameTranslation": "The Dawn",
          "revelationType": "Meccan",
          "numberOfAyahs": 5
        },
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6228,
        "text": "ومن شر ليل شديد الظلمة إذا دخل وتغلغل، وما فيه من الشرور والمؤذيات.",
        "surah": {
          "number": 113,
          "name": "سُورَةُ الفَلَقِ",
          "englishName": "Al-Falaq",
          "englishNameTranslation": "The Dawn",
          "revelationType": "Meccan",
          "numberOfAyahs": 5
        },
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6229,
        "text": "ومن شر الساحرات اللاتي ينفخن فيما يعقدن من عُقَد بقصد السحر.",
        "surah": {
          "number": 113,
          "name": "سُورَةُ الفَلَقِ",
          "englishName": "Al-Falaq",
          "englishNameTranslation": "The Dawn",
          "revelationType": "Meccan",
          "numberOfAyahs": 5
        },
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6230,
        "text": "ومن شر حاسد مبغض للناس إذا حسدهم على ما وهبهم الله من نعم، وأراد زوالها عنهم، وإيقاع الأذى بهم.",
        "surah": {
          "number": 113,
          "name": "سُورَةُ الفَلَقِ",
          "englishName": "Al-Falaq",
          "englishNameTranslation": "The Dawn",
          "revelationType": "Meccan",
          "numberOfAyahs": 5
        },
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 555,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6231,
        "text": "قل -أيها الرسول-: أعوذ وأعتصم برب الناس، القادر وحده على ردِّ شر الوسواس.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6232,
        "text": "ملك الناس المتصرف في كل شؤونهم، الغنيِّ عنهم.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6233,
        "text": "إله الناس الذي لا معبود بحق سواه.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6234,
        "text": "من أذى الشيطان الذي يوسوس عند الغفلة، ويختفي عند ذكر الله.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6235,
        "text": "الذي يبثُّ الشر والشكوك في صدور الناس.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
          "englishNameTranslation": "Mankind",
          "revelationType": "Meccan",
          "numberOfAyahs": 6
        },
        "numberInSurah": 5,
        "juz": 30,
        "manzil": 7,
        "page": 604,
        "ruku": 556,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6236,
        "text": "من شياطين الجن والإنس.",
        "surah": {
          "number": 114,
          "name": "سُورَةُ النَّاسِ",
          "englishName": "An-Naas",
