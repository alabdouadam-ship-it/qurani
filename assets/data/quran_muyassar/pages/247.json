        "sajda": false
      },
      {
        "number": 1694,
        "text": "قال يعقوب: سوف أسأل ربي أن يغفر لكم ذنوبكم، إنه هو الغفور لذنوب عباده التائبين، الرحيم بهم.",
        "surah": {
          "number": 12,
          "name": "سُورَةُ يُوسُفَ",
          "englishName": "Yusuf",
          "englishNameTranslation": "Joseph",
          "revelationType": "Meccan",
          "numberOfAyahs": 111
        },
        "numberInSurah": 98,
