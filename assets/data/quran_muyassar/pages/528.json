        "hizbQuarter": 211,
        "sajda": false
      },
      {
        "number": 4851,
        "text": "هذا القرآن الذي جاءهم حكمة عظيمة بالغة غايتها، فأي شيء تغني النذر عن قوم أعرضوا وكذَّبوا بها؟",
        "surah": {
          "number": 54,
          "name": "سُورَةُ القَمَرِ",
          "englishName": "Al-Qamar",
          "englishNameTranslation": "The Moon",
          "revelationType": "Meccan",
          "numberOfAyahs": 55
        },
        "numberInSurah": 5,
        "juz": 27,
        "manzil": 7,
        "page": 528,
