        "hizbQuarter": 208,
        "sajda": false
      },
      {
        "number": 4667,
        "text": "إن في إهلاك القرون الماضية لعبرة لمن كان له قلب يعقل به، أو أصغى السمع، وهو حاضر بقلبه، غير غافل ولا ساهٍ.",
        "surah": {
          "number": 50,
          "name": "سُورَةُ قٓ",
          "englishName": "Qaaf",
          "englishNameTranslation": "The letter Qaaf",
          "revelationType": "Meccan",
          "numberOfAyahs": 45
        },
        "numberInSurah": 37,
        "juz": 26,
        "manzil": 7,
        "page": 520,
