    "ayahs": [
      {
        "number": 1443,
        "text": "وقال فرعون: جيئوني بكل ساحر متقن للسحر.",
        "surah": {
          "number": 10,
          "name": "سُورَةُ يُونُسَ",
          "englishName": "Yunus",
          "englishNameTranslation": "Jonas",
