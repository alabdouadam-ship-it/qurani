        "page": 602,
        "ruku": 548,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6197,
        "text": "الذي أطعمهم من جوع شديد، وآمنهم من فزع وخوف عظيم.",
        "surah": {
          "number": 106,
          "name": "سُورَةُ قُرَيۡشٍ",
          "englishName": "Quraish",
          "englishNameTranslation": "Quraysh",
          "revelationType": "Meccan",
          "numberOfAyahs": 4
        },
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 548,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6198,
        "text": "أرأيت حال ذلك الذي يكذِّب بالبعث والجزاء؟",
        "surah": {
          "number": 107,
          "name": "سُورَةُ المَاعُونِ",
          "englishName": "Al-Maa'un",
          "englishNameTranslation": "Almsgiving",
          "revelationType": "Meccan",
          "numberOfAyahs": 7
        },
        "numberInSurah": 1,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6199,
        "text": "فذلك الذي يدفع اليتيم بعنف وشدة عن حقه؛ لقساوة قلبه.",
        "surah": {
          "number": 107,
          "name": "سُورَةُ المَاعُونِ",
          "englishName": "Al-Maa'un",
          "englishNameTranslation": "Almsgiving",
          "revelationType": "Meccan",
          "numberOfAyahs": 7
        },
        "numberInSurah": 2,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6200,
        "text": "ولا يحضُّ غيره على إطعام المسكين، فكيف له أن يطعمه بنفسه؟",
        "surah": {
          "number": 107,
          "name": "سُورَةُ المَاعُونِ",
          "englishName": "Al-Maa'un",
          "englishNameTranslation": "Almsgiving",
          "revelationType": "Meccan",
          "numberOfAyahs": 7
        },
        "numberInSurah": 3,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6201,
        "text": "فعذاب شديد للمصلين الذين هم عن صلاتهم لاهون، لا يقيمونها على وجهها، ولا يؤدونها في وقتها.",
        "surah": {
          "number": 107,
          "name": "سُورَةُ المَاعُونِ",
          "englishName": "Al-Maa'un",
          "englishNameTranslation": "Almsgiving",
          "revelationType": "Meccan",
          "numberOfAyahs": 7
        },
        "numberInSurah": 4,
        "juz": 30,
        "manzil": 7,
        "page": 602,
        "ruku": 549,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6202,
        "text": "فعذاب شديد للمصلين الذين هم عن صلاتهم لاهون، لا يقيمونها على وجهها، ولا يؤدونها في وقتها.",
        "surah": {
          "number": 107,
          "name": "سُورَةُ المَاعُونِ",
          "englishName": "Al-Maa'un",
          "englishNameTranslation": "Almsgiving",
          "revelationType": "Meccan",
          "numberOfAyahs": 7
        },
        "numberInSurah": 5,
        "juz": 30,
