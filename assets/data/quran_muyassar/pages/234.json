        "hizbQuarter": 94,
        "sajda": false
      },
      {
        "number": 1586,
        "text": "ولا تميلوا إلى هؤلاء الكفار الظلمة، فتصيبكم النار، وما لكم من دون الله من ناصر ينصركم، ويتولى أموركم.",
        "surah": {
          "number": 11,
          "name": "سُورَةُ هُودٍ",
          "englishName": "Hud",
          "englishNameTranslation": "Hud",
          "revelationType": "Meccan",
          "numberOfAyahs": 123
        },
        "numberInSurah": 113,
        "juz": 12,
