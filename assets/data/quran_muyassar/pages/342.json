    "ayahs": [
      {
        "number": 2674,
        "text": "قد فاز المصدِّقون بالله وبرسوله العاملون بشرعه.",
        "surah": {
          "number": 23,
          "name": "سُورَةُ المُؤۡمِنُونَ",
          "englishName": "Al-Muminoon",
          "englishNameTranslation": "The Believers",
          "revelationType": "Meccan",
          "numberOfAyahs": 118
        },
        "numberInSurah": 1,
        "juz": 18,
