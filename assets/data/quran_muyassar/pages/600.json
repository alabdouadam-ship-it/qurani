        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6165,
        "text": "وأما من خفت موازين حسناته، ورجحت موازين سيئاته، فمأواه جهنم.",
        "surah": {
          "number": 101,
          "name": "سُورَةُ القَارِعَةِ",
          "englishName": "Al-Qaari'a",
          "englishNameTranslation": "The Calamity",
          "revelationType": "Meccan",
          "numberOfAyahs": 11
        },
        "numberInSurah": 8,
        "juz": 30,
        "manzil": 7,
        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6166,
        "text": "وأما من خفت موازين حسناته، ورجحت موازين سيئاته، فمأواه جهنم.",
        "surah": {
          "number": 101,
