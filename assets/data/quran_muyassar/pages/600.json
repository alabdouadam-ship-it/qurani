        "page": 600,
        "ruku": 543,
        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6168,
        "text": "إنها نار قد حَمِيت من الوقود عليها.",
        "surah": {
          "number": 101,
          "name": "سُورَةُ القَارِعَةِ",
          "englishName": "Al-Qaari'a",
          "englishNameTranslation": "The Calamity",
          "revelationType": "Meccan",
          "numberOfAyahs": 11
        },
        "numberInSurah": 11,
        "juz": 30,
