        "hizbQuarter": 240,
        "sajda": false
      },
      {
        "number": 6167,
        "text": "وما أدراك -أيها الرسول- ما هذه الهاوية؟",
        "surah": {
          "number": 101,
          "name": "سُورَةُ القَارِعَةِ",
          "englishName": "Al-Qaari'a",
          "englishNameTranslation": "The Calamity",
          "revelationType": "Meccan",
          "numberOfAyahs": 11
        },
        "numberInSurah": 10,
        "juz": 30,
