    "ayahs": [
      {
        "number": 2302,
        "text": "ونادينا موسى من ناحية جبل طور \"سيناء\" اليمنى من موسى، وقرَّبناه فشرَّفناه بمناجاتنا له. وفي هذا إثبات صفة الكلام لله - تعالى - كما يليق بجلاله وكماله.",
        "surah": {
          "number": 19,
          "name": "سُورَةُ مَرۡيَمَ",
          "englishName": "Maryam",
          "englishNameTranslation": "Mary",
          "revelationType": "Meccan",
          "numberOfAyahs": 98
        },
        "numberInSurah": 52,
        "juz": 16,
        "manzil": 4,
        "page": 309,
