    "ayahs": [
      {
        "number": 2701,
        "text": "فإذا علوت السفينة مستقرًا عليها أنت ومن معك آمنين من الغرق، فقل: الحمد لله الذي نجَّانا من القوم الكافرين.",
        "surah": {
          "number": 23,
          "name": "سُورَةُ المُؤۡمِنُونَ",
          "englishName": "Al-Muminoon",
          "englishNameTranslation": "The Believers",
          "revelationType": "Meccan",
          "numberOfAyahs": 118
        },
        "numberInSurah": 28,
        "juz": 18,
        "manzil": 4,
        "page": 344,
