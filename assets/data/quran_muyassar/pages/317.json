    "ayahs": [
      {
        "number": 2425,
        "text": "ولقد أوحينا إلى موسى: أن اخرُج ليلا بعبادي من بني إسرائيل من \"مصر\"، فاتِّخِذْ لهم في البحر طريقًا يابسًا، لا تخاف من فرعون وجنوده أن يلحقوكم فيدركوكم، ولا تخشى في البحر غرقًا.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 77,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2426,
        "text": "فأسرى موسى ببني إسرائيل، وعبر بهم طريقًا في البحر، فأتبعهم فرعون بجنوده، فغمرهم من الماء ما لا يعلم كنهه إلا الله، فغرقوا جميعًا ونجا موسى وقومه.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 78,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2427,
        "text": "وأضلَّ فرعون قومه بما زيَّنه لهم من الكفر والتكذيب، وما سلك بهم طريق الهداية.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 79,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2428,
        "text": "يا بني إسرائيل اذكروا حين أنجيناكم مِن عدوكم فرعون، وجَعَلْنا موعدكم بجانب جبل الطور الأيمن لإنزال التوراة عليكم، ونزلنا عليكم في التيه ما تأكلونه، مما يشبه الصَّمغ طعمه كالعسل، والطير الذي يشبه السُّمَانَى.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 80,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2429,
        "text": "كلوا من رزقنا الطيب، ولا تعتدوا فيه بأن يظلم بعضكم بعضًا، فينزل بكم غضبي، ومَن ينزل به غضبي فقد هلك وخسر.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 81,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2430,
        "text": "وإني لَغفار لمن تاب من ذنبه وكفره، وآمن بي وعمل الأعمال الصالحة، ثم اهتدى إلى الحق واستقام عليه.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 82,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 126,
        "sajda": false
      },
      {
        "number": 2431,
        "text": "وأيُّ شيء أعجلك عن قومك - يا موسى - فسبقتَهم إلى جانب الطور الأيمن، وخلَّفتَهم وراءك؟",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 83,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2432,
        "text": "قال: إنهم خلفي سوف يلحقون بي، وسبقتُهم إليك - يا ربي - لتزداد عني رضا.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 84,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2433,
        "text": "قال الله لموسى: فإنا قد ابتلينا قومك بعد فراقك إياهم بعبادة العجل، وإن السامري قد أضلهم.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 85,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2434,
        "text": "فرجع موسى إلى قومه غضبان عليهم حزينًا، وقال لهم: يا قوم ألم يَعِدْكم ربكم وعدًا حسنًا بإنزال التوراة؟ أفطال عليكم العهد واستبطأتم الوعد، أم أردتم أن تفعلوا فعلا يحل عليكم بسببه غضب من ربكم، فأخلفتم موعدي وعبدتم العجل، وتركتم الالتزام بأوامري؟",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 86,
        "juz": 16,
        "manzil": 4,
        "page": 317,
        "ruku": 273,
        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2435,
        "text": "قالوا: يا موسى ما أخلفنا موعدك باختيارنا، ولكنَّا حُمِّلنا أثقالا مِن حليِّ قوم فرعون، فألقيناها في حفرة فيها نار بأمر السامري، فكذلك ألقى السامري ما كان معه من تربة حافر فرس جبريل عليه السلام.",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
