        "hizbQuarter": 127,
        "sajda": false
      },
      {
        "number": 2437,
        "text": "أفلا يرى الذين عبدوا العجل أنه لا يكلمهم ابتداء، ولا يردُّ عليهم جوابًا، ولا يقدر على دفع ضرٍّ عنهم، ولا جلب نفع لهم؟",
        "surah": {
          "number": 20,
          "name": "سُورَةُ طه",
          "englishName": "Taa-Haa",
          "englishNameTranslation": "Taa-Haa",
          "revelationType": "Meccan",
          "numberOfAyahs": 135
        },
        "numberInSurah": 89,
        "juz": 16,
