        "hizbQuarter": 131,
        "sajda": false
      },
      {
        "number": 2560,
        "text": "ونصرناه مِن كيد القوم الذين كذَّبوا بآياتنا الدالة على صدقه، إنهم كانوا أهل قُبْح، فأغرقناهم بالطوفان أجمعين.",
        "surah": {
          "number": 21,
          "name": "سُورَةُ الأَنبِيَاءِ",
          "englishName": "Al-Anbiyaa",
          "englishNameTranslation": "The Prophets",
          "revelationType": "Meccan",
          "numberOfAyahs": 112
        },
        "numberInSurah": 77,
