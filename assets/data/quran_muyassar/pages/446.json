        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3794,
        "text": "إنَّا زينَّا السماء الدنيا بزينة هي النجوم.",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
          "englishName": "As-Saaffaat",
          "englishNameTranslation": "Those drawn up in Ranks",
          "revelationType": "Meccan",
          "numberOfAyahs": 182
        },
        "numberInSurah": 6,
        "juz": 23,
