        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3801,
        "text": "وإذا ذكِّروا بما نسوه أو غَفَلوا عنه لا ينتفعون بهذا الذكر ولا يتدبَّرون.",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
          "englishName": "As-Saaffaat",
          "englishNameTranslation": "Those drawn up in Ranks",
          "revelationType": "Meccan",
          "numberOfAyahs": 182
        },
        "numberInSurah": 13,
        "juz": 23,
