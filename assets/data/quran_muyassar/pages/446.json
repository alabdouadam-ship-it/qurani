        "hizbQuarter": 178,
        "sajda": false
      },
      {
        "number": 3808,
        "text": "وقالوا: يا هلاكنا هذا يوم الحساب والجزاء.",
        "surah": {
          "number": 37,
          "name": "سُورَةُ الصَّافَّاتِ",
          "englishName": "As-Saaffaat",
