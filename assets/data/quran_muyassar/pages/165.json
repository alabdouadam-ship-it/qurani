        "sajda": false
      },
      {
        "number": 1076,
        "text": "وهو رب موسى وهارون، وهو الذي يجب أن تصرف له العبادة وحده دون مَن سواه.",
        "surah": {
          "number": 7,
          "name": "سُورَةُ الأَعۡرَافِ",
          "englishName": "Al-A'raaf",
          "englishNameTranslation": "The Heights",
          "revelationType": "Meccan",
          "numberOfAyahs": 206
        },
        "numberInSurah": 122,
        "juz": 9,
        "manzil": 2,
        "page": 165,
