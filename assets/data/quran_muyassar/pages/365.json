    "ayahs": [
      {
        "number": 2911,
        "text": "وما أرسلناك - أيها الرسول - إلا مبشرًا للمؤمنين بالجنة ومنذرًا للكافرين بالنار.",
        "surah": {
          "number": 25,
          "name": "سُورَةُ الفُرۡقَانِ",
          "englishName": "Al-Furqaan",
          "englishNameTranslation": "The Criterion",
          "revelationType": "Meccan",
