        "hizbQuarter": 121,
        "sajda": false
      },
      {
        "number": 2229,
        "text": "ثم رجع ذو القرنين إلى المشرق متبعًا الأسباب التي أعطاه الله إياها.",
        "surah": {
          "number": 18,
          "name": "سُورَةُ الكَهۡفِ",
          "englishName": "Al-Kahf",
          "englishNameTranslation": "The Cave",
          "revelationType": "Meccan",
