    "ayahs": [
      {
        "number": 4506,
        "text": "وظهر لهؤلاء الذين كانوا يكذِّبون بآيات الله ما عملوا في الدنيا من الأعمال القبيحة، ونزل بهم من عذاب الله جزاء ما كانوا به يستهزئون.",
        "surah": {
          "number": 45,
          "name": "سُورَةُ الجَاثِيَةِ",
          "englishName": "Al-Jaathiya",
          "englishNameTranslation": "Crouching",
          "revelationType": "Meccan",
          "numberOfAyahs": 37
        },
        "numberInSurah": 33,
        "juz": 25,
